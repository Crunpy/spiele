DROP DATABASE IF EXISTS Spiele;

CREATE DATABASE Spiele;

USE Spiele;

CREATE TABLE Spiel
(
	ID INT PRIMARY KEY,
    Name VARCHAR(50),
    Geb DATE
);

INSERT INTO Spiel(ID, Name, Geb) VALUES
